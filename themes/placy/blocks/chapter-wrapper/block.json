{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "placy/chapter-wrapper",
  "title": "Tema Story Kapittel",
  "category": "layout",
  "icon": "book",
  "description": "Wrapper for tema story kapitler med automatisk chapter ID",
  "keywords": ["chapter", "kapittel", "tema", "story"],
  "supports": {
    "align": false,
    "html": false
  },
  "attributes": {
    "chapterId": {
      "type": "string",
      "default": ""
    },
    "chapterAnchor": {
      "type": "string",
      "default": ""
    },
    "chapterTitle": {
      "type": "string",
      "default": ""
    }
  },
  "editorScript": "file:./block.js",
  "render": "file:./render.php"
}
