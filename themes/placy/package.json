{
  "name": "placy",
  "version": "1.0.0",
  "description": "Placy WordPress Theme",
  "main": "index.js",
  "scripts": {
    "build:css": "tailwindcss -i ./css/tailwind-input.css -o ./css/tailwind-output.css --minify",
    "watch:css": "tailwindcss -i ./css/tailwind-input.css -o ./css/tailwind-output.css --watch",
    "build:js": "terser js/tema-story-map-multi.js -o js/tema-story-map-multi.min.js -c -m --source-map",
    "build:blocks": "./build-blocks.sh",
    "build": "npm run build:css && npm run build:js && npm run build:blocks",
    "lint": "eslint js/*.js --ignore-pattern '*.min.js'",
    "lint:fix": "eslint js/*.js --ignore-pattern '*.min.js' --fix"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@tailwindcss/typography": "^0.5.19",
    "autoprefixer": "^10.4.22",
    "eslint": "^9.39.1",
    "globals": "^16.5.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.18",
    "terser": "^5.44.1"
  }
}
